    key <AD07> { [            l,            L,               lstroke,               Lstroke,      less,            KP_7,               Greek_lambda,       Greek_LAMBDA       ] };
    key <AD08> { [            u,            U,            udiaeresis,                Uacute,      greater,         KP_8,               Greek_upsilon,      U222A              ] };
    key <AD09> { [            y,            Y,                uacute,            Udiaeresis,      equal,           KP_9,               Greek_xi,           U27C2              ] };
    key <AD10> { [    semicolon,        colon,                oacute,            Odiaeresis,      ampersand,       KP_Add,             U22C5,   		   multiply           ] };
