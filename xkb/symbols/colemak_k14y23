    key <AD09> { [            y,            Y,           udiaeresis,            Udiaeresis,    equal,                NoSymbol,     Greek_phi,       Greek_PHI         ] };
    key <AD10> { [    semicolon,        colon,           odiaeresis,            Odiaeresis,    ampersand,            NoSymbol,     NoSymbol,        NoSymbol          ] };
