    key <AC01> { [            a,            A,            adiaeresis,                Aacute,      backslash,       Home,               Greek_alpha,        U2200              ] };
    key <AC02> { [            r,            R,            dead_grave,            asciitilde,      slash,           Left,               Greek_rho,          U211D              ] };
    key <AC03> { [            s,            S,                scaron,                Scaron,      braceleft,       Down,               Greek_sigma,        U2211              ] };
    key <AC04> { [            t,            T,            dead_acute,      dead_doubleacute,      braceright,      Right,              Greek_tau,          U22A5              ] };
    key <AC05> { [            g,            G,           dead_ogonek,            asciitilde,      asterisk,        End,                Greek_gamma,        Greek_GAMMA        ] };
