// Colemak-DH (ANSI) but with added 3rd modifier (layer 5),
// taken from 3rd modifier from DE-Neo.

xkb_symbols "colemak_k14y23" {
    include "us(colemak_dh)"
    
    name[Group1]= "English (K14y23)";
    
    key.type[Group1] = "ONE_LEVEL";
    key <CAPS> { [ Control_L ] }; 

    // Apostrophe key is Level-5 modifer:
    key <AC11> { [ ISO_Level5_Shift ] };
	modifier_map Mod3 { <AC11> };

    key.type[Group1]= "EIGHT_LEVEL_ALPHABETIC";

    // LAYERS:           NORMAL         SHIFT                ALT-GR           SHIFT+ALT-GR     MOD3                  MOD3+SHIFT    MOD3+ALT-GR      MOD3+SHIFT+ALT-GR
    key <AE01> { [            1,       exclam,           exclamdown,           onesuperior,    onesubscript,         NoSymbol,     onesubscript,    NoSymbol          ] };
    key <AE02> { [            2,           at,            masculine,           twosuperior,    twosubscript,         NoSymbol,     twosubscript,    NoSymbol          ] };
    key <AE03> { [            3,   numbersign,          ordfeminine,         threesuperior,    threesubscript,       NoSymbol,     threesubscript,  NoSymbol          ] };
    key <AE04> { [            4,       dollar,                 cent,              sterling,    U2039,                NoSymbol,     femalesymbol,    NoSymbol          ] };
    key <AE05> { [            5,      percent,             EuroSign,                   yen,    U203A,                NoSymbol,     malesymbol,      NoSymbol          ] };
