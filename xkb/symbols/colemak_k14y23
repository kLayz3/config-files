    key <AE08> { [            8,     asterisk,                thorn,                 THORN,    malesymbol,           NoSymbol,     U27E8,           NoSymbol          ] };
    key <AE09> { [            9,    parenleft,  leftsinglequotemark,   leftdoublequotemark,    femalesymbol,         NoSymbol,     U27E9,           NoSymbol          ] };
    key <AE10> { [            0,   parenright, rightsinglequotemark,  rightdoublequotemark,    U26A5,                NoSymbol,     zerosubscript,   NoSymbol          ] };

    key <AD01> { [            q,            Q,           adiaeresis,            Adiaeresis,    Escape,               NoSymbol,     Greek_xi,        Greek_XI          ] };
