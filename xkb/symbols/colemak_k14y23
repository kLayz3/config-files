// Colemak-DH (ANSI) but with added 3rd modifier (layer 5),
// taken from 3rd modifier from DE-Neo.

xkb_symbols "colemak_k14y23" {
    include "us(colemak_dh)"
    
    name[Group1]= "English (K14y23)";
    
    key.type[Group1] = "ONE_LEVEL";
    key <CAPS> { [ Control_L ] }; 
	modifier_map Control { <CAPS> };

    // Apostrophe and Slash keys are Level-5 modifer:
    key <AC11> { [ ISO_Level5_Shift ] };
	modifier_map Mod3 { <AC11> };
	key <BKSL> { [ ISO_Level5_Shift ] };
	modifier_map Mod3 { <BKSL> };

    key.type[Group1]= "EIGHT_LEVEL_ALPHABETIC";

    // LAYERS:           NORMAL         SHIFT                 ALT-GR           SHIFT+ALT-GR       MOD3             MOD3+SHIFT          MOD3+ALT-GR         MOD3+SHIFT+ALT-GR
    key <AE01> { [            1,       exclam,            exclamdown,           onesuperior,      onesubscript,    ordfeminine,        onesubscript,       notsign            ] };
    key <AE02> { [            2,           at,             masculine,           twosuperior,      twosubscript,    masculine,          twosubscript,       logicaland         ] };
    key <AE03> { [            3,   numbersign,           ordfeminine,         threesuperior,      threesubscript,  numerosign,         threesubscript,     logicalor          ] };
    key <AE04> { [            4,       dollar,                  cent,              sterling,      U2039,           NoSymbol,           copyright,          implies            ] };
    key <AE05> { [            5,      percent,              EuroSign,                   yen,      U203A,           periodcentered,     registered,         ifonlyif           ] };
    key <AE06> { [            6,  asciicircum,            dead_caron,            asciitilde,      cent,            sterling,           trademark,          U2203              ] };
    key <AE07> { [            7,    ampersand,                   eth,                   ETH,      yen,             currency,           U03F0,              U221E              ] };
    key <AE08> { [            8,     asterisk,                 thorn,                 THORN,      malesymbol,      Tab,                U27E8,              variation          ] };
    key <AE09> { [            9,    parenleft,   leftsinglequotemark,   leftdoublequotemark,      femalesymbol,    KP_Divide,          U27E9,              emptyset           ] };
    key <AE10> { [            0,   parenright,  rightsinglequotemark,  rightdoublequotemark,      U26A5,           KP_Multiply,        zerosubscript,      intersection       ] };
                                                                                                                                                                      
    key <AD01> { [            q,            Q,                aacute,            Adiaeresis,      Escape,          Prior,              U2235,              U211A              ] };
